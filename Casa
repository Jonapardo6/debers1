public abstract class Casa extends InmuebleVivienda {
    protected int numeroPisos;

    public Casa(int identificadorInmobiliario, int area, String direccion, double valorPorMetroCuadrado, int numeroHabitaciones, int numeroBanos, int numeroPisos) {
        super(identificadorInmobiliario, area, direccion, valorPorMetroCuadrado, numeroHabitaciones, numeroBanos);
        this.numeroPisos = numeroPisos;
    }

    @Override
    public void imprimir() {
        super.imprimir();
        System.out.println("NÃºmero de Pisos: " + numeroPisos);
    }
}
